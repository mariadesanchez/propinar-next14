generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}// Order de compras
model Order {
  id           String    @id @default(uuid())
  total        Float
  isPaid       Boolean   @default(false)
  paidAt       DateTime?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

}